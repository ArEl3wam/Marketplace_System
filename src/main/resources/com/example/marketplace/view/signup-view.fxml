<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.marketplace.controller.SignUpController">
   <children>
      <BorderPane>
         <left>
            <Button mnemonicParsing="false" onAction="#onBack" prefHeight="42.0" prefWidth="100.0" text="Back" BorderPane.alignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </left>
      </BorderPane>
      <BorderPane prefHeight="100.0" prefWidth="1000.0">
         <center>
            <TextField fx:id="name" prefWidth="440.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
         </center>
         <left>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name" wrappingWidth="160.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="23.0" />
               </font>
            </Text>
         </left>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </BorderPane>
      <BorderPane prefHeight="100.0" prefWidth="1000.0">
         <center>
            <TextField fx:id="email" prefHeight="38.0" prefWidth="803.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
         </center>
         <left>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email" wrappingWidth="160.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="23.0" />
               </font>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Text>
         </left>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </BorderPane>
      <BorderPane prefHeight="100.0" prefWidth="200.0">
         <left>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password" wrappingWidth="160.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="23.0" />
               </font>
            </Text>
         </left>
         <center>
            <PasswordField fx:id="password" prefWidth="440.0" BorderPane.alignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </PasswordField>
         </center>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </BorderPane>
      <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="307.0" spacing="30.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#onSignUpButtonClick" prefHeight="40.0" prefWidth="163.0" text="Sign up">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
